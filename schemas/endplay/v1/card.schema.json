    "$id": "https://dominicprice.github.io/schemas/endplay/v1/card.schema.json",
    "title": "Card",
    "type": "object",
    "description": "A playing card",
    "properties": {
        "suit": {
            "$ref": "https://dominicprice.github.io/schemas/endplay/v1/denom.schema.json",
